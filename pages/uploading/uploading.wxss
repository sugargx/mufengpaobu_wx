.pc1::after{
  width: 79rpx;
  height: 4rpx;
}
.pc1::before,.pc1::after{
  content:" ";
  position:absolute;
  top:50%;
  left:50%;
  -webkit-transform:translate(-50%, -50%);
  transform:translate(-50%, -50%);
  background-color:#D9D9D9;

}
.pc1::before{
  width: 2rpx;
  height: 79rpx;
}
