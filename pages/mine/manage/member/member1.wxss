.content{
  width:100%;
  height: 100%;
  flex-direction: column;
  padding-top: 20rpx;
}
.head{
  padding: 30rpx;
  text-align: center;
  color: skyblue;
  font-size: 40rpx;
}
.headpic{
  border-radius: 34rpx;
}
page{
  background-color: #eee;
}
.body{
  width: 750rpx;
  height: 96rpx;
  display: flex;
  background-color: #fff;
  border-bottom: 2rpx solid #ccc;
  padding-left: 20rpx;
  position: relative;
}
.memberInfo{
  width: 450rpx;
  height: 68rpx;
  padding-left: 20rpx;
  font-size: 40rpx;
  line-height: 68rpx;
  box-sizing: border-box;
  margin-top: 14rpx;
}
.btn-del{
  width:160rpx;
  height: 96rpx;
  border-radius: 0rpx;
  line-height: 80rpx;
  position: absolute;
  top:0;
  right:0;
  /* text-align: center; */
}
.delImg{
  height: 80rpx;
  width: 80rpx;
}
.all{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 10;
  background-color: rgba(0,0,0,0.70);
}
.s1{
  height: 70rpx;
  line-height: 70rpx;
}
.info{
  background-color: #fff;
  margin: 100rpx auto;
  width: 640rpx;
  height: 600rpx;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}
.title{
  text-align: center;
  font-size: 50rpx;
  line-height: 80rpx;
  height: 100rpx;
  border-bottom: 2rpx solid #ccc;
}
.main{
  color: #666;
  font-size: 40rpx;
  line-height: 60rpx;
  height: 60rpx;
}
.close{
  background-color: #fff;
  font-size: 35rpx;
  line-height: 80rpx;
  height: 80rpx;
  position: absolute;
  bottom: 0;
  width: 100%;
  left: 0;
  border-top: 2rpx solid #6e6;
  text-align: center;
  color: #6e6;
}
.xx{
  color: #fff;
  background-color: #6e6;
  line-height: 80rpx;
  font-size: 40rpx;
  width: 80rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 30rpx
}

image{
  width: 60rpx;
  height: 60rpx;
  margin-top: 0rpx;
  border-radius: 50rpx;
  margin-left: 10rpx;
}
/* pages/leftSwiperDel/index.wxss */

.item-box{
    width: 700rpx;
    margin: 0 auto;
    padding:40rpx 0;
}
.items{
    width: 100%;
}
.item{
    position: relative;
    border-top: 2rpx solid #eee;
    height: 100rpx;
    line-height: 100rpx;
    overflow: hidden;
    
}
.item:last-child{
    border-bottom: 2rpx solid #eee;
}
.inner{
    position: absolute;
    top:0;
}
.inner.txt{
    background-color: #fff;
    width: 100%;
    z-index: 5;
    height: 100rpx;
    border-top:1rpx solid #e5e5e5; 
    line-height: 100rpx;
    font-size: 32rpx;
    transition: left 0.2s ease-in-out;
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
}
.inner.del{
    background-color: #e64340;
    width: 180rpx;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    z-index: 4;
    right: 0;
    color: #fff
}
.item-icon{
    width: 64rpx;
    vertical-align: middle;
    margin-right: 16rpx
}
.thumb{
    width: 200px;
    height: 200px;
    -webkit-overflow-scrolling: touch;
    overflow: scroll;
}